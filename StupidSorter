                            echo -e "\e[1;37mОтлично, перемещаем в\e[0m \e[1;32mвидеофайлы\e[0m \e[1;37mиз\e[0m \e[1;32m$source_dir\e[0m \e[1;37mв\e[0m \e[1;32m$vid_dir\e[0m"
                            echo "  "
                        vibor_vid_dir=1
                        else
                            echo "  "
                            echo -e "\e[0;31mТакой директории не существует, проверьте правильность написания\e[0m"
                            echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'vid'\e[0m \e[0;33m**\e[0m"
                        fi
                    else
                        echo "  "
                        echo -e "\e[0;33m**Введите дирректорию, в которую хотите перенести отсортированные файлы**\e[0m"
                        echo -e "\e[0;33m**Дирректория должна быть в формате '/home/user/folder/'**\e[0m"
                        echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'vid'\e[0m \e[0;33m**\e[0m"
                        echo -e "\e[0;33m**Для выхода в главное меню введите\e[0m \e[0;36m'reset'\e[0m \e[0;33m**\e[0m"
                    fi
                done
                fi
                pustota=0
                echo "  "
                for i in ${!ras_vid[@]}; do
                    extension="${ras_vid[$i]}"
                    if [ "$(find $source_dir -maxdepth 1 -type f -name "*$extension" | wc -l)" -gt 0 ]; then
                        find $source_dir -maxdepth 1 -type f -name "*$extension" -exec mv {} $vid_dir \;
                        echo -e "\e[0;32mФайлы с расширением\e[0m \e[0;36m$extension\e[0m \e[0;32mуспешно перемещены\e[0m"
                    else
                        ((pustota++))
                    fi
                done
                a=i
                ((a++))
                if [ "$pustota" -eq $a ]; then
                    echo -e "\e[0;31mК сожалению в директории\e[0m \e[0;36m$source_dir\e[0m \e[0;31mне было найдено ни одного видеофайла\e[0m"
                fi   
                echo "  "
                echo -e "\e[1;37mГотовимся к сортировке\e[0m \e[1;32mАрхивов\e[0m"
                vibor_zip_dir=0
                if [[ $auto_vvod == "y" ]]; then
                    cd 
                    cd Загрузки
                    mkdir zip
                    cd zip
                    zip_dir=$(pwd)
                    cd ..
                    cd ..
                    cd Загрузки
                else
                    while [[ $vibor_zip_dir -ne 1 ]]; do 
                    echo "   "
                    echo -e "\e[1;36mВыберете директорию, куда будут перенесены файлы:\e[0m"
                    read zip_dir
                    if [[ $zip_dir == "reset" ]]; then
                        echo "exit"
                        exec "$0"
                    fi
                    if [[ $zip_dir != "help" ]]; then
                        if [[ $zip_dir == "zip" ]]; then
                            cd 
                            cd Загрузки
                            mkdir zip
                            cd zip
                            zip_dir=$(pwd)
                            cd ..
                            cd ..
                            cd Загрузки
                        fi
                        if [ -d "$zip_dir" ]; then
                            echo "  "
                            echo -e "\e[1;37mОтлично, перемещаем в\e[0m \e[1;32mархивы\e[0m \e[1;37mиз\e[0m \e[1;32m$source_dir\e[0m \e[1;37mв\e[0m \e[1;32m$zip_dir\e[0m"
                            echo "  "
                        vibor_zip_dir=1
                        else
                            echo "  "
                            echo -e "\e[0;31mТакой директории не существует, проверьте правильность написания\e[0m"
                            echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'zip'\e[0m \e[0;33m**\e[0m"
                        fi
                    else
                        echo "  "
                        echo -e "\e[0;33m**Введите дирректорию, в которую хотите перенести отсортированные файлы**\e[0m"
                        echo -e "\e[0;33m**Дирректория должна быть в формате '/home/user/folder/'**\e[0m"
                        echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'zip'\e[0m \e[0;33m**\e[0m"
                        echo -e "\e[0;33m**Для выхода в главное меню введите\e[0m \e[0;36m'reset'\e[0m \e[0;33m**\e[0m"
                    fi
                done
                fi
                pustota=0
                echo "  "
                for i in ${!ras_zip[@]}; do
                    extension="${ras_zip[$i]}"
                    if [ "$(find $source_dir -maxdepth 1 -type f -name "*$extension" | wc -l)" -gt 0 ]; then
                        find $source_dir -maxdepth 1 -type f -name "*$extension" -exec mv {} $zip_dir \;
                        echo -e "\e[0;32mФайлы с расширением\e[0m \e[0;36m$extension\e[0m \e[0;32mуспешно перемещены\e[0m"
                    else
                        ((pustota++))
                    fi
                done
                a=i
                ((a++))
                if [ "$pustota" -eq $a ]; then
                    echo -e "\e[0;31mК сожалению в директории\e[0m \e[0;36m$source_dir\e[0m \e[0;31mне было найдено ни одного архива\e[0m"
                fi   
                echo "  "
                echo -e "\e[1;37mГотовимся к сортировке\e[0m \e[1;32mУстановочных файлов\e[0m"
                vibor_exe_dir=0
                if [[ $auto_vvod == "y" ]]; then
                    cd 
                    cd Загрузки
                    mkdir exe
                    cd exe
                    exe_dir=$(pwd)
                    cd ..
                    cd ..
                    cd Загрузки
                else
                    while [[ $vibor_exe_dir -ne 1 ]]; do 
                    echo "   "
                    echo -e "\e[1;36mВыберете директорию, куда будут перенесены файлы:\e[0m"
                    read exe_dir
                    if [[ $exe_dir == "reset" ]]; then
                        echo "exit"
                        exec "$0"
                    fi
                    if [[ $exe_dir != "help" ]]; then
                        if [[ $exe_dir == "exe" ]]; then
                            cd 
                            cd Загрузки
                            mkdir exe
                            cd exe
                            exe_dir=$(pwd)
                            cd ..
                            cd ..
                            cd Загрузки
                        fi
                        if [ -d "$exe_dir" ]; then
                            echo "  "
                            echo -e "\e[1;37mОтлично, перемещаем в\e[0m \e[1;32mустановочные файлы\e[0m \e[1;37mиз\e[0m \e[1;32m$source_dir\e[0m \e[1;37mв\e[0m \e[1;32m$exe_dir\e[0m"
                            echo "  "
                        vibor_exe_dir=1
                        else
                            echo "  "
                            echo -e "\e[0;31mТакой директории не существует, проверьте правильность написания\e[0m"
                            echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'exe'\e[0m \e[0;33m**\e[0m"
                        fi
                    else
                        echo "  "
                        echo -e "\e[0;33m**Введите дирректорию, в которую хотите перенести отсортированные файлы**\e[0m"
                        echo -e "\e[0;33m**Дирректория должна быть в формате '/home/user/folder/'**\e[0m"
                        echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'exe'\e[0m \e[0;33m**\e[0m"
                        echo -e "\e[0;33m**Для выхода в главное меню введите\e[0m \e[0;36m'reset'\e[0m \e[0;33m**\e[0m"
                    fi
                done
                fi
                pustota=0
                echo "  "
                for i in ${!ras_exe[@]}; do
                    extension="${ras_exe[$i]}"
                    if [ "$(find $source_dir -maxdepth 1 -type f -name "*$extension" | wc -l)" -gt 0 ]; then
                        find $source_dir -maxdepth 1 -type f -name "*$extension" -exec mv {} $exe_dir \;
                        echo -e "\e[0;32mФайлы с расширением\e[0m \e[0;36m$extension\e[0m \e[0;32mуспешно перемещены\e[0m"
                    else
                        ((pustota++))
                    fi
                done
                a=i
                ((a++))
                if [ "$pustota" -eq $a ]; then
                    echo -e "\e[0;31mК сожалению в директории\e[0m \e[0;36m$source_dir\e[0m \e[0;31mне было найдено ни одного установочного файла\e[0m"
                fi   
                echo "  " 
                echo -e "\e[1;37mГотовимся к сортировке\e[0m \e[1;32mОбразов дисков\e[0m"
                vibor_iso_dir=0
                if [[ $auto_vvod == "y" ]]; then
                    cd 
                    cd Загрузки
                    mkdir iso
                    cd iso
                    iso_dir=$(pwd)
                    cd ..
                    cd ..
                    cd Загрузки
                else
                    while [[ $vibor_iso_dir -ne 1 ]]; do 
                    echo "   "
                    echo -e "\e[1;36mВыберете директорию, куда будут перенесены файлы:\e[0m"
                    read iso_dir
                    if [[ $iso_dir == "reset" ]]; then
                        echo "exit"
                        exec "$0"
                    fi
                    if [[ $iso_dir != "help" ]]; then
                        if [[ $iso_dir == "iso" ]]; then
                            cd 
                            cd Загрузки
                            mkdir iso
                            cd iso
                            iso_dir=$(pwd)
                            cd ..
                            cd ..
                            cd Загрузки
                        fi
                        if [ -d "$iso_dir" ]; then
                            echo "  "
                            echo -e "\e[1;37mОтлично, перемещаем в\e[0m \e[1;32mобразы дисков\e[0m \e[1;37mиз\e[0m \e[1;32m$source_dir\e[0m \e[1;37mв\e[0m \e[1;32m$iso_dir\e[0m"
                            echo "  "
                        vibor_iso_dir=1
                        else
                            echo "  "
                            echo -e "\e[0;31mТакой директории не существует, проверьте правильность написания\e[0m"
                            echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'iso'\e[0m \e[0;33m**\e[0m"
                        fi
                    else
                        echo "  "
                        echo -e "\e[0;33m**Введите дирректорию, в которую хотите перенести отсортированные файлы**\e[0m"
                        echo -e "\e[0;33m**Дирректория должна быть в формате '/home/user/folder/'**\e[0m"
                        echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'iso'\e[0m \e[0;33m**\e[0m"
                        echo -e "\e[0;33m**Для выхода в главное меню введите\e[0m \e[0;36m'reset'\e[0m \e[0;33m**\e[0m"
                    fi
                done
                fi
                pustota=0
                echo "  "
                for i in ${!ras_iso[@]}; do
                    extension="${ras_iso[$i]}"
                    if [ "$(find $source_dir -maxdepth 1 -type f -name "*$extension" | wc -l)" -gt 0 ]; then
                        find $source_dir -maxdepth 1 -type f -name "*$extension" -exec mv {} $iso_dir \;
                        echo -e "\e[0;32mФайлы с расширением\e[0m \e[0;36m$extension\e[0m \e[0;32mуспешно перемещены\e[0m"
                    else
                        ((pustota++))
                    fi
                done
                a=i
                ((a++))
                if [ "$pustota" -eq $a ]; then
                    echo -e "\e[0;31mК сожалению в директории\e[0m \e[0;36m$source_dir\e[0m \e[0;31mне было найдено ни одного образа дисков\e[0m"
                fi   
                echo "  " 
                echo -e "\e[1;37mГотовимся к сортировке\e[0m \e[1;32mФайлов языков программирования\e[0m"
                vibor_lang_dir=0
                if [[ $auto_vvod == "y" ]]; then
                    cd 
                    cd Загрузки
                    mkdir lang
                    cd lang
                    lang_dir=$(pwd)
                    cd ..
                    cd ..
                    cd Загрузки
                else
                    while [[ $vibor_lang_dir -ne 1 ]]; do 
                    echo "   "
                    echo -e "\e[1;36mВыберете директорию, куда будут перенесены файлы:\e[0m"
                    read lang_dir
                    if [[ $lang_dir == "reset" ]]; then
                        echo "exit"
                        exec "$0"
                    fi
                    if [[ $lang_dir != "help" ]]; then
                        if [[ $lang_dir == "lang" ]]; then
                            cd 
                            cd Загрузки
                            mkdir lang
                            cd lang
                            lang_dir=$(pwd)
                            cd ..
                            cd ..
                            cd Загрузки
                        fi
                        if [ -d "$lang_dir" ]; then
                            echo "  "
                            echo -e "\e[1;37mОтлично, перемещаем в\e[0m \e[1;32mфайлы языков программирования\e[0m \e[1;37mиз\e[0m \e[1;32m$source_dir\e[0m \e[1;37mв\e[0m \e[1;32m$lang_dir\e[0m"
                            echo "  "
                        vibor_lang_dir=1
                        else
                            echo "  "
                            echo -e "\e[0;31mТакой директории не существует, проверьте правильность написания\e[0m"
                            echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'lang'\e[0m \e[0;33m**\e[0m"
                        fi
                    else
                        echo "  "
                        echo -e "\e[0;33m**Введите дирректорию, в которую хотите перенести отсортированные файлы**\e[0m"
                        echo -e "\e[0;33m**Дирректория должна быть в формате '/home/user/folder/'**\e[0m"
                        echo -e "\e[0;33m**Если вы хотите использовать директорию по умолчанию, введите\e[0m \e[0;36m'lang'\e[0m \e[0;33m**\e[0m"
                        echo -e "\e[0;33m**Для выхода в главное меню введите\e[0m \e[0;36m'reset'\e[0m \e[0;33m**\e[0m"
                    fi
                done
                fi
                pustota=0
                echo "  "
                for i in ${!ras_lang[@]}; do
                    extension="${ras_lang[$i]}"
                    if [ "$(find $source_dir -maxdepth 1 -type f -name "*$extension" | wc -l)" -gt 0 ]; then
                        find $source_dir -maxdepth 1 -type f -name "*$extension" -exec mv {} $lang_dir \;
                        echo -e "\e[0;32mФайлы с расширением\e[0m \e[0;36m$extension\e[0m \e[0;32mуспешно перемещены\e[0m"
                    else
                        ((pustota++))
                    fi
                done
                a=i
                ((a++))
                if [ "$pustota" -eq $a ]; then
                    echo -e "\e[0;31mК сожалению в директории\e[0m \e[0;36m$source_dir\e[0m \e[0;31mне было найдено ни одного файла языков программирования\e[0m"
                fi   
                echo "  "
                exitt=0
                while [[ $exitt -ne 1 ]]; do 
                    echo -e "\e[1;32mСортировка окончена!\e[0m \e[1;36mПерезапустить программу? (y/exit) \e[0m"
                    read exitexit
                    if [[ $exitexit == "exit" ]]; then
                        echo "Выход"
                        exit 0
                    elif [[ $exitexit == "y" ]]; then
                        echo "Перезагрузка"
                        exec "$0"
                    else
                        echo "  "
                        echo -e "\e[1;33mДля продолжения выберете один из вариантов\e[0m"
                        echo "  "
                    fi
                done
            ;;
        esac
    done
done